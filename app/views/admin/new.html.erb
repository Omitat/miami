<h1> Sign Up </h1>

<%= form_for @admin, url: admin_path do |f| %>
<p>
  <%= f.label :email %>
  <%= f.text_field :email %>
</p>
<p>
  <%= f.label :password_digest %>
  <%= f.password_field :password_digest %>
</p>
<p>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
</p>
<p>
<%= f.submit %>
</p>
<% end %>
